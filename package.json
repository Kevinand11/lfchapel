    "build:functions": "npm run build --prefix ./firebase/functions",
    "lint:functions": "npm run lint --prefix ./firebase/functions",
    "config:functions:get": "firebase functions:config:get > firebase/functions/.runtimeconfig.json",
    "config:functions:set": "firebase functions:config:set"
  },
