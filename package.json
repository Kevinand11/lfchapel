  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start:firebase": "npm run build:functions && cross-env NODE_ENV=development firebase emulators:start --import ./docs/exports",
